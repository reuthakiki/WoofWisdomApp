<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/mainLogo"
        android:layout_width="394dp"
        android:layout_height="311dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="409dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <ImageButton
        android:id="@+id/mapsBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="320dp"
        android:layout_marginBottom="322dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainLogo"
        app:srcCompat="@android:drawable/ic_dialog_map" />

    <TextView
        android:id="@+id/nearVet"
        android:layout_width="62dp"
        android:layout_height="34dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="320dp"
        android:layout_marginBottom="278dp"
        android:text="Nearest Vet"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapsBtn" />

    <ImageButton
        android:id="@+id/vaccinationsBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="316dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/mapsBtn"
        app:layout_constraintTop_toBottomOf="@+id/mainLogo"
        app:srcCompat="@android:drawable/ic_menu_edit" />

    <TextView
        android:id="@+id/vaccinationRecord"
        android:layout_width="95dp"
        android:layout_height="59dp"
        android:layout_marginStart="59dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="276dp"
        android:text="Immunization Record"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.807"
        app:layout_constraintStart_toEndOf="@+id/nearVet"
        app:layout_constraintTop_toBottomOf="@+id/vaccinationsBtn"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/cameraAnalyzer"
        android:layout_width="60dp"
        android:layout_height="56dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="322dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vaccinationsBtn"
        app:layout_constraintTop_toBottomOf="@+id/mainLogo"
        app:srcCompat="@android:drawable/ic_menu_camera" />

    <TextView
        android:id="@+id/stoolPukeAnalyzer"
        android:layout_width="88dp"
        android:layout_height="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="259dp"
        android:text="Stool and Puke analyzer"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/vaccinationRecord"
        app:layout_constraintTop_toBottomOf="@+id/cameraAnalyzer" />

    <ImageButton
        android:id="@+id/suspiciousFoodBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="316dp"
        android:layout_marginBottom="187dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nearVet"
        app:srcCompat="@android:drawable/ic_dialog_alert" />

    <TextView
        android:id="@+id/dangerousFood"
        android:layout_width="90dp"
        android:layout_height="53dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="292dp"
        android:layout_marginBottom="134dp"
        android:text="Suspicious Food Analyzer"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/suspiciousFoodBtn" />

    <ImageButton
        android:id="@+id/forumsBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="173dp"
        android:layout_marginBottom="182dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dangerousFood"
        app:layout_constraintTop_toBottomOf="@+id/vaccinationRecord"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <TextView
        android:id="@+id/forums"
        android:layout_width="118dp"
        android:layout_height="65dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="172dp"
        android:layout_marginBottom="160dp"
        android:text="Forums With Proffessionals"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.433"
        app:layout_constraintStart_toEndOf="@+id/dangerousFood"
        app:layout_constraintTop_toBottomOf="@+id/forumsBtn"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/dogBreedsInfoBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="185dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/forumsBtn"
        app:layout_constraintTop_toBottomOf="@+id/stoolPukeAnalyzer"
        app:srcCompat="@android:drawable/ic_dialog_info" />

    <TextView
        android:id="@+id/dogBreedsInfo"
        android:layout_width="74dp"
        android:layout_height="45dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="135dp"
        android:text="Dog Breeds Information"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/forums"
        app:layout_constraintTop_toBottomOf="@+id/dogBreedsInfoBtn" />

</androidx.constraintlayout.widget.ConstraintLayout>